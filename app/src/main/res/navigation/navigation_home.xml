<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_home"
    app:startDestination="@id/blank_screen">
    <fragment
        android:id="@+id/blank_screen"
        android:name="com.example.myapplication.BlankFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_blank" >
        <action
            android:id="@+id/action_navigation_home_to_parksFragment"
            app:destination="@id/parksFragment" />
        <action
            android:id="@+id/action_navigation_home_to_cafeFragment"
            app:destination="@id/cafeFragment" />
        <action
            android:id="@+id/action_navigation_home_to_gunFragment"
            app:destination="@id/gunFragment" />
        <action
            android:id="@+id/action_navigation_home_to_hotelsFragment"
            app:destination="@id/hotelsFragment" />
        <action
            android:id="@+id/action_navigation_home_to_itemFragment"
            app:destination="@id/itemFragment" />
    </fragment>

    <fragment
        android:id="@+id/itemFragment"
        android:name="com.example.myapplication.ItemFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_item" />

    <fragment
        android:id="@+id/parksFragment"
        android:name="com.example.myapplication.ParksFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_parks" />

    <fragment
        android:id="@+id/hotelsFragment"
        android:name="com.example.myapplication.HotelsFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_hotels" />

    <fragment
        android:id="@+id/cafeFragment"
        android:name="com.example.myapplication.CafeFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_cafe" />


    <fragment
        android:id="@+id/gunFragment"
        android:name="com.example.myapplication.GunFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_gun" />

</navigation>