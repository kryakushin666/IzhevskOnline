            android:layout_alignParentTop="true"
            android:layout_marginLeft="69dp"
            android:layout_marginTop="329dp"
            android:src="@drawable/ic_cafe" />

        <ImageView
            android:id="@+id/button2"
            android:layout_width="120dp"
